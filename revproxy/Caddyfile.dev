http://localhost:4200 {
    reverse_proxy /graphql* http://api:80
    reverse_proxy /swagger* http://api:80
    reverse_proxy frontend:8080 {
        header_up Host {host}
        header_up X-Forwarded-For {remote}
        header_up X-Forwarded-Port {server_port}
        header_up X-Forwarded-Proto {scheme}
    }
}